{
  "name": "app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --config nodemon.json --watch 'src/**/*.ts' --exec \"ts-node\" src/server.ts",
    "clean": "rm -rf build",
    "compile": "tsc -p tsconfig.json",
    "build": "npm run clean && npm run compile",
    "lint": "eslint . -c .eslintrc.js --ext .ts --ignore-path .eslintignore",
    "lint:fix": "eslint . -c .eslintrc.js --ext .ts --ignore-path .eslintignore --fix",
    "typeorm": "typeorm-ts-node-commonjs",
    "typeorm-cli": "ts-node node_modules/typeorm/cli.js --dataSource src/db/dataSource.ts",
    "db:sync": "ts-node node_modules/typeorm/cli.js migration:generate -p ./src/db/migrations/sync --dataSource ./src/db/dataSource.ts",
    "db:migrate:up": "ts-node node_modules/typeorm/cli.js migration:run --dataSource ./src/db/dataSource.ts",
    "db:migrate:down": "ts-node node_modules/typeorm/cli.js migration:revert --dataSource ./src/db/dataSource.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/yup": "^0.29.14",
    "body-parser": "^1.20.0",
    "concurrently": "^7.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "kill-port": "^2.0.1",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "0.3.7",
    "uuid": "^8.3.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.4.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^16.11.10",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "eslint": "^8.19.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "nodemon": "^2.0.18",
    "ts-node": "10.7.0",
    "typescript": "4.5.2"
  }
}
